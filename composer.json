{
    "name": "nulxrd/hades",
    "description": "A high-performance PHP database abstract layer with Object-Oriented API and more many features.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "database",
        "mysql",
        "mariadb",
        "pgsql",
        "postgresql",
        "querybuilder",
        "pdo",
        "sql",
        "sqlite"
    ],
    "support": {
        "issues": "https://github.com/nulxrd/hades/issues",
        "source": "https://github.com/nulxrd/hades"
    },
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "jetbrains/phpstorm-attributes": "dev-master",
        "phpstan/phpstan": "2.0.x-dev",
        "phpunit/phpunit": "^11.1.x-dev",
        "squizlabs/php_codesniffer": "4.0.x-dev"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Hades\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hades\\Tests\\Unit\\": "tests/unit/",
            "Hades\\Tests\\Stubs\\": "tests/stubs/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "8.2.19"
        },
        "sort-packages": true
    }
}
